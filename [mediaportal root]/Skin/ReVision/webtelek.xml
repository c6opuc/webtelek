<window>
  <id>6900</id>
  <defaultcontrol>1</defaultcontrol>
  <allowoverlay>yes</allowoverlay>
  <autohidetopbar>yes</autohidetopbar>
  <define>#header.label:WEBTELEK+</define>
  <define>#header.hover:hover_webtelek.png</define>
  <controls>
    <import>common.window.xml</import>
    <import>common.time.xml</import>
		<control>
			<description>Logo</description>
			<type>image</type>
			<id>2</id>
			<posX>5</posX>
			<posY>35</posY>
			<width>300</width>
      			<keepaspectratio>yes</keepaspectratio>
			<texture>webtelek_logotype.gif</texture>
		</control>
    		<control>
    			<description>ChannelLogo</description>
      			<type>image</type>
      			<id>6</id>
      			<posX>332</posX>
      			<posY>18</posY>
      			<width>74</width>
      			<height>74</height>
      			<texture></texture>
      			<keepaspectratio>yes</keepaspectratio>
      			<centered>yes</centered>
      			<animation effect="fade" time="200">WindowOpen</animation>
      			<animation effect="fade" time="200">WindowClose</animation>
    		</control>
		<control>
			<type>group</type>
			<description>group element</description>			
			<posX>10</posX>
			<posY>130</posY>
			<layout>StackLayout</layout>
			<control>
				<description>Favorites</description>
				<type>button</type>
				<id>7</id>
				<posX>20</posX>
				<posY>150</posY>
				<label>#favorites</label>
				<align>left</align>
				<font>font14</font>
			</control>
			<control>
				<description>Select Country</description>
				<type>button</type>
				<id>4</id>
				<posX>20</posX>
				<posY>200</posY>
				<label>#select</label>
				<onleft>4</onleft>      			
				<onright>10</onright>
				<onup>7</onup>
				<ondown>9</ondown>
				<align>left</align>
				<font>font14</font>
			</control>
			<control>
				<description>Select category</description>
				<type>button</type>
				<id>9</id>
				<posX>20</posX>
				<posY>250</posY>      			
				<label>#archive</label>
				<font>font14</font>
				<align>left</align>
			</control>
			<control>
      				<description>Kinozal</description>
      				<type>button</type>
      				<id>3</id>
      				<posX>20</posX>
      				<posY>300</posY>      			
      				<label>#kinozal</label>
      				<align>left</align>
				<font>font14</font>
    			</control>
			<control>
      				<description>Others</description>
      				<type>button</type>
      				<id>8</id>
      				<posX>20</posX>
      				<posY>350</posY>      			
      				<label>#others</label>
      				<ondown>99</ondown>
      				<align>left</align>
				<font>font14</font>
    			</control>
		</control>
		<control>
			<description>Current category</description>
			<type>label</type>
			<id>5</id>
			<posX>325</posX>
			<posY>120</posY>
			<label>#Header</label>
			<font>font18</font>
			<align>left</align>
			<textcolor>ffffffff</textcolor>
		</control>
		<control>
      			<description>Channels</description>
      			<type>listcontrol</type>
      			<id>10</id>
			<posX>325</posX>
      			<posY>170</posY>
      			<width>1025</width>
      			<height>500</height>
			<textXOff>30</textXOff>
			<textYOff>2</textYOff>
			<textXOff2>478</textXOff2>
			<textYOff2>2</textYOff2>
			<textXOff3>486</textXOff3>
			<textYOff3>2</textYOff3>
      			<spaceBetweenItems>8</spaceBetweenItems>
      			<textureHeight>35</textureHeight>
      			<keepaspectratio>no</keepaspectratio>
      			<onleft>7</onleft>
      			<onright>7</onright>
			<font>font14</font>
		</control>
		<control>
   			<type>textboxscrollup</type>
      			<description>Channel-Info</description>
      			<id>11</id>
      			<posX>15</posX>
      			<posY>310</posY>
      			<width>300</width>
      			<height>280</height>
			<font>font14</font>
      			<label>#ChannelInfo</label>
      			<textcolor>ffffffff</textcolor>
		</control>
	</controls>
</window>